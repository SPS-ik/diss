<UserControl x:Class="FIMAE.Views.SideMenu.SideMenuProcessing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
	Height="535" 
	Width="240">

    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="64"/>
            <RowDefinition Height="64"/>
        </Grid.RowDefinitions>
        <Button Content="&lt;-"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                FontSize="18"
                Margin="24,16,24,8"
                Width="24"
                Command="{Binding BackCommand}"/>
        <TextBlock Text="Обробка ГІМАС" TextWrapping="Wrap" HorizontalAlignment="Right" VerticalAlignment="Top" FontSize="18" Margin="24,16,24,8"/>
        <StackPanel Grid.Row="1" Orientation="Vertical">
            <TextBlock Text="Вхідне значення" TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="14" Margin="24,8,24,4"/>
            <ComboBox Margin="24,4,24,8"/>
        </StackPanel>
        <Button Grid.Row="2" Content="Обробка" Margin="24,8"/>
    </Grid>
</UserControl>
